		"@testing-library/user-event": "^14.5.2",
		"@types/eslint": "^9.6.0",
		"@typescript-eslint/eslint-plugin": "^8.11.0",
		"@typescript-eslint/parser": "^8.11.0",
		"@vitest/coverage-v8": "^4.0.18",
